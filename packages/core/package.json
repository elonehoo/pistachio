{
  "name": "@elonehoo/pistachio",
  "version": "0.0.6",
  "license": "MIT",
  "description": "out-of-the-box ready to use composables",
  "author": "Elone Hoo <huchengyea@163.com>",
  "homepage": "https://github.com/elonehoo/pistachio#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elonehoo/pistachio.git"
  },
  "bugs": {
    "url": "https://github.com/elonehoo/pistachio/issues"
  },
  "keywords": [
    "vue",
    "composables-api",
    "utils"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": "./*"
  },
  "files": [
    "dist",
    "*.d.ts"
  ],
  "scripts": {
    "build": "rimraf dist && tsup",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "vue": ">=3.2.0"
  },
  "devDependencies": {
    "@vue/compiler-dom": "3.2.47",
    "@vue/devtools-api": "6.5.0",
    "@vue/runtime-dom": "3.2.47",
    "@vue/shared": "3.2.47",
    "tsup": "^6.7.0",
    "unbuild": "^1.2.1",
    "vue": "3.2.47"
  }
}
